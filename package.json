{
  "name": "vuejs-blog-generator",
  "version": "3.0.0",
  "private": true,
  "description": "Blog generator template",
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "overrides": {
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/escape-html": "^1.0.1",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.26",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^0.7.6",
    "escape-html": "^1.0.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.3.2",
    "prismjs": "^1.27.0",
    "sass": "^1.49.8",
    "typescript": "~4.6.3",
    "vite": "^2.9.14",
    "vite-ssg": "^0.20.2",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.33.9"
  }
}
